#include<iostream>
#include<vector>
using namespace std;
//int reversearay(vector<int>arr){
  //  int n = arr.size();

//}
int main(){
    int i,j,n;
cout<< "Enter the no. of element in vector:" << endl;
cin>> n;
vector<int> arr(n);
cout<< "Enter the element of vector:"<< endl;
for(i=0;i<arr.size();i++){
cin>> arr[i];
}
for(i=0;i<arr.size();i++){
cout<< arr[i] << endl;
}
  for ( i = 0, j = arr.size() - 1; i < j; i++, j--)  
    {     
       int temp = arr[i];  
        arr[i] = arr[j];  
        arr[j] = temp;  
    }  
    for(i=0;i<arr.size();i++){
cout<< arr[i] << " " ;
}
cout << endl;
}
