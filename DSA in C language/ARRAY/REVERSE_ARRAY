#include<stdio.h>

int main() {
    int arr[100], n, i, j; 
    printf("Enter the length of array = ");
    scanf("%d", &n);

    printf("Enter the elements of array = ");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Array in reverse order: ");
    for (j = n - 1; j >= 0; j--) { 
        printf("%d ", arr[j]);
    }

    return 0;
}
