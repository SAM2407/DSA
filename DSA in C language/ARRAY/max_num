#include<stdio.h>
int main(){
    int arr[100],n,i,max=0;
     printf("Enter the length of array = ");
    scanf("%d", &n);
    printf("Enter the element of array = ");
    for (i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }
     for (i = 0; i < n; i++)
    {
        if(arr[i]>max){
            max= arr[i];
        }
    }
printf("%d",max);
}
