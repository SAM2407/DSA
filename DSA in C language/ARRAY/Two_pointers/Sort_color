#include<iostream>
using namespace std;
 void sort(int arr[100],int n){
  int low = 0, mid = 0, high = n - 1;

    while(mid <= high){
        if(arr[mid] == 0){
            swap(arr[low], arr[mid]);
            low++;
            mid++;
        }
        else if(arr[mid] == 1){
            mid++;
        }
        else { 
            swap(arr[mid], arr[high]);
            high--;
        }
    }
}


int main(){
    int n,arr[1000];
    cout<<"Enter the size of array:-"<<endl;
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
 for(int i=0;i<n;i++){
        cout<<arr[i];
    }
    cout<<"array after soering"<<endl;
    sort(arr,n);
   
    return 0;
}
